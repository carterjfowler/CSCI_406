import java.util.ArrayList;

public class node {
	point location;
	dfs discovery;
	point parent;
	ArrayList<point> blackEdges;
	ArrayList<point> redEdges;
	
	public node(point _location, dfs _discovery, point _parent, ArrayList<point> _blackEdges, ArrayList<point> _redEdges) {
		location = _location;
		discovery = _discovery;
		parent = _parent;
		blackEdges = _blackEdges;
		redEdges = _redEdges;
	}
}
