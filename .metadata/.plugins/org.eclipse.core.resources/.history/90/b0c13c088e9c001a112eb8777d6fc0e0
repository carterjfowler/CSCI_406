import java.util.ArrayList;

public class mazeNode {
	point location;
	ArrayList<point> blackEdges;
	ArrayList<point> redEdges;
	
	public mazeNode(point _location, ArrayList<point> _blackEdges, ArrayList<point> _redEdges) {
		location = _location;
		blackEdges = _blackEdges;
		redEdges = _redEdges;
	}
}
