import java.util.ArrayList;

public class node {
	point location;
	dfs discovery;
	ArrayList<point> blackEdges;
	ArrayList<point> redEdges;
	
	public node(point _location, dfs _discovery, ArrayList<point> _blackEdges, ArrayList<point> _redEdges) {
		location = _location;
		discovery = _discover;
		blackEdges = _blackEdges;
		redEdges = _redEdges;
	}
}
